    "js": [
      "https://unpkg.com/kintone-ui-component/umd/kuc.min.js",
      "js/desktop.js"
    ],
    "css": [
      "https://unpkg.com/kintone-ui-component/umd/kuc.min.css", 
      "css/51-modern-default.css",
      "css/desktop.css"
    ]
  },
  "icon": "image/icon.png",
